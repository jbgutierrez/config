    <item>
      <name>Navigation Layer</name>
      <identifier>private.navigation_layer</identifier>
      <autogen>
        __KeyOverlaidModifier__
        KeyCode::SPACE,

        @begin
        KeyCode::VK_CONFIG_FORCE_ON_private_navigation_layer_space,
        KeyCode::VK_MODIFIER_NAVIGATION_LAYER,
        @end

        @begin
        KeyCode::SPACE,,
        @end
      </autogen>
