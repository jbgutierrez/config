    <item hidden="true">
      <identifier vk_config="true">private.navigation_layer_space</identifier>
      <modifier_only>ModifierFlag::NAVIGATION_LAYER</modifier_only>
      <autogen>
        __BlockUntilKeyUp__ KeyCode::SPACE,
      </autogen>
      <autogen>
        __KeyToKey__ KeyCode::H, KeyCode::CURSOR_LEFT,

        Option::KEYTOKEY_INCREASE_MODIFIER_FLAGS,
        ModifierFlag::NAVIGATION_LAYER,
